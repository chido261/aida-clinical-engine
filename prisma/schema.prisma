generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
}

model UserState {
  id                 String   @id @default("demo-user")
  createdAt          DateTime @default(now())
  updatedAt          DateTime @updatedAt

  baselineA1c         Float?
  baselineAvgGlucose  Int?
  baselineSetAt       DateTime?
}



model Reading {
  id        Int      @id @default(autoincrement())
  userId    String
  glucose   Int
  moment    String
  symptoms  String?
  createdAt DateTime @default(now())
}
